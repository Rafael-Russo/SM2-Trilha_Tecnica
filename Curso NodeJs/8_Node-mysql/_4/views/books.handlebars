<h1>Livros disponíveis: </h1>
<div class="books-container">
  <a href="/">Adicionar livro</a>
  {{#each books}}
    <div>
      #{{this.idbooks}}
      -
      <a href="/book/{{this.idbooks}}"><span
          class="bold"
        >{{this.title}}</span></a>
      -
      {{this.pageqtd}}
      páginas
    </div>
  {{/each}}
</div>